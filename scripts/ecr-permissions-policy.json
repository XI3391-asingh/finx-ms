{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "ecr-repository-policy",
            "Effect": "Allow",
            "Principal": {
                "AWS": [
                "arn:aws:iam::840087460680:root"
                ]
            },
            "Action": [
                "ecr:GetDownloadUrlForLayer",
                "ecr:BatchGetImage",
                "ecr:BatchCheckLayerAvailability",
                "ecr:PutImage",
                "ecr:InitiateLayerUpload",
                "ecr:UploadLayerPart",
                "ecr:CompleteLayerUpload"
            ]
        }
    ]
}

